file = {:Users/joosep/Library/Application Support/Mendeley Desktop/Downloaded/Lepage - 1978 - A New Algorithm for Adaptive Multidimensional Integration.pdf:pdf},
journal = {JOURNAL OF COMPUTATIONAL PHYSICS},
pages = {192--203},
title = {{A New Algorithm for Adaptive Multidimensional Integration}},
url = {http://ac.els-cdn.com/0021999178900049/1-s2.0-0021999178900049-main.pdf?{\_}tid=cf6f2680-93ca-11e7-a1c2-00000aab0f26{\&}acdnat=1504788673{\_}44cb12b03effeb7c00ecd053cc9b620a},
