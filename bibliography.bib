title = {{Evidence for the 125 GeV Higgs boson decaying to a pair of $\tau$ leptons}},
url = {http://link.springer.com/10.1007/JHEP05(2014)104{\%}5Cnhttp://arxiv.org/abs/1401.5041{\%}5Cnhttp://cds.cern.ch/record/1643937},
